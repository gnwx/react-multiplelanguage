{
    "name": "react-multiplelanguage",
    "version": "1.0.0",
    "description": "Easy to use translation for React",
    "main": "dist/cjs/index.js",
    "types": "dist/esm/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rm -rf dist/ && prettier --write src/ && npm run build:esm && npm run build:cjs",
        "build:esm": "tsc",
        "build:cjs": "tsc --module CommonJS --outDir dist/cjs"
    },
    "keywords": [
        "react",
        "package"
    ],
    "author": "gnw",
    "license": "MIT",
    "peerDependencies": {
        "react": "^18.0.2"
    },
    "devDependencies": {
        "@types/node": "^20.2.5",
        "@types/react": "^18.2.8",
        "@types/styled-components": "^5.1.26",
        "prettier": "^2.8.8",
        "react": "^18.0.2",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "styled-components": "^5.3.11"
    }
}
